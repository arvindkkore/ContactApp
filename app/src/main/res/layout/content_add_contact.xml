<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_add_contact"
        tools:context=".persenter.view.ui.activity.AddContactActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
           android:layout_height="match_parent"
            android:layout_width="match_parent"
            >
<androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
       >

    <EditText
            android:layout_width="286dp"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress|textPersonName"
            android:text="First Name"
            android:ems="10"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginTop="16dp" android:layout_marginStart="16dp"
            android:id="@+id/edittext_name"/>
    <EditText
            android:layout_width="286dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="LastName"
            android:ems="10"
            app:layout_constraintStart_toStartOf="@+id/edittext_name"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:id="@+id/edittext_lastname"
            app:layout_constraintTop_toBottomOf="@+id/edittext_name"
            app:layout_constraintEnd_toEndOf="@+id/edittext_name"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginEnd="1dp" android:layout_marginRight="1dp"/>
    <EditText
            android:layout_width="281dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:text="Phone Number"
            android:ems="10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:id="@+id/edittext_phonenumber"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/edittext_lastname"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Phone Type"
            android:ems="10"
            android:id="@+id/edittext_phone_type"
            app:layout_constraintEnd_toEndOf="@+id/edittext_phonenumber"
            app:layout_constraintStart_toStartOf="@+id/edittext_phonenumber" android:layout_marginEnd="7dp"
            android:layout_marginRight="7dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_phonenumber" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Email"
            android:ems="10"
            app:layout_constraintStart_toStartOf="@+id/edittext_phone_type"
            android:id="@+id/edittext_email" app:layout_constraintEnd_toEndOf="@+id/edittext_phone_type"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/edittext_phone_type"
            app:layout_constraintHorizontal_bias="0.475"/>
    <EditText
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Email Type"
            android:ems="10"
            app:layout_constraintStart_toStartOf="@+id/edittext_email"
            app:layout_constraintEnd_toEndOf="@+id/edittext_email" android:id="@+id/edittext_emailtype"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_email"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Address"
            android:ems="10"
            android:id="@+id/edittext_address" app:layout_constraintEnd_toEndOf="@+id/edittext_emailtype"
            app:layout_constraintStart_toStartOf="@+id/edittext_emailtype" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_emailtype" app:layout_constraintHorizontal_bias="0.504"
    />
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Address"
            android:ems="10"
            android:id="@+id/edittext_address_type"
            app:layout_constraintEnd_toEndOf="@+id/edittext_address"
            app:layout_constraintStart_toStartOf="@+id/edittext_address"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_address"
            app:layout_constraintHorizontal_bias="0.504"
    />

    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Website"
            android:ems="10"
            android:id="@+id/edittext_website"
            app:layout_constraintEnd_toEndOf="@+id/edittext_address_type"
            app:layout_constraintStart_toStartOf="@+id/edittext_address_type"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_address_type"
            app:layout_constraintHorizontal_bias="0.504"
    />

    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Website Type"
            android:ems="10"
            android:id="@+id/edittext_website_type"
            app:layout_constraintEnd_toEndOf="@+id/edittext_website"
            app:layout_constraintStart_toStartOf="@+id/edittext_website"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_website"
            app:layout_constraintHorizontal_bias="0.504"
    />

    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Note"
            android:ems="10"
            android:id="@+id/edittext_note"
            app:layout_constraintEnd_toEndOf="@+id/edittext_website_type"
            app:layout_constraintStart_toStartOf="@+id/edittext_website_type"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/edittext_website_type"
            app:layout_constraintHorizontal_bias="0.504"
    />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>